{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .ts,.vue --cache --cache-location node_modules/.cache/.eslintcache",
    "format": "npm run lint -- --fix; prettier . --cache --write --log-level warn",
    "format-check": "prettier --cache --ignore-unknown --check .",
    "type": "npx -y nuxi typecheck",
    "type-gen": "openapi-typescript http://localhost:4000/api-json -o ./types/index.ts",
    "test": "vitest run",
    "test:open": "vitest --ui"
  },
  "devDependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@invictus.codes/nuxt-vuetify": "^0.3.0",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@types/node": "^18.17.3",
    "@vitest/ui": "^0.34.2",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "happy-dom": "^10.10.4",
    "javascript-time-ago": "^2.5.9",
    "nuxt": "^3.6.5",
    "nuxt-vitest": "^0.10.2",
    "openapi-typescript": "^6.5.0",
    "prettier": "^3.0.2",
    "sweetalert2": "^11.7.27",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "openapi-fetch": "^0.7.4"
  }
}
